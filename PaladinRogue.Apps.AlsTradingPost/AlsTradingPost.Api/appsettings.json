{
    "Logging": {
        "IncludeScopes": false,
        "Debug": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    },
    "ProxySettings": {
        "UseProxy": true
    },
    "FacebookSettings": {
        "AccessTokenValidationEndpoint": "https://graph.facebook.com/debug_token?input_token={0}&access_token={1}",
        "DataEndpoint": "https://graph.facebook.com/v2.8/me?fields=first_name,last_name,name,locale,picture&access_token={model.AccessToken}"
    }
}
